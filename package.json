    "crypto-js": "^4.1.1",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "markdown-it-link-attributes": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.3",
    "vue-tsc": "^1.6.5"
