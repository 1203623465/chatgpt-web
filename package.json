    "markdown-it": "^13.0.1",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@iconify/vue": "^4.1.1",
